Take_input1()
{
clear
while :
do
clear                   
echo -e "${WHITE}                ____ "
echo -e "${RED}    _________  /  _/___  ___  _____"
echo -e "${WHITE}   / ___/ __ \ / // __ \/ _ \/ ___/"
echo -e "${RED}  (__  ) / / // // /_/ /  __/ /    "
echo -e "${WHITE} /____/_/ /_/___/ .___/\___/_/     "
echo -e "${RED}               /_/                 "
echo -e "${WHITE} "
echo -e "${WHITE}  "${WHITE}
    echo -e "${WHITE}CODED BY ÇOLAK ${GREEN}DarkSecArmy.COM | PcDunyasi.TV${RED}  [NO EDIT] "
    echo -e "${WHITE} "${GREEN}CTRL + Z YAPARAK SCRIPTDEN CIKIS YAPABILIRSINIZ
    echo -e "${RED}==================== PARAMETRELER ====================+ "
    echo -e "${LIGHTBLUE}[1]${WHITE}GIT CLONE SNIPER${LIGHTBLUE}                          "    	
    echo -e "${LIGHTBLUE}[2]${WHITE}SN1PER KURULUM${LIGHTBLUE}                          " 
    echo -e "${LIGHTBLUE}[3]${WHITE}NORMAL MOD${LIGHTBLUE}                        "
    echo -e "${LIGHTBLUE}[4]${WHITE}NORMAL MOD + OSINT + RECON${LIGHTBLUE}                        "
    echo -e "${LIGHTBLUE}[5]${WHITE}STEALTH MOD + OSINT + RECON${LIGHTBLUE}                         "
    echo -e "${LIGHTBLUE}[6]${WHITE}KESFETME MOD${LIGHTBLUE}                          "     
    echo -e "${LIGHTBLUE}[7]${WHITE}ÖZEL PORT TARAMA${LIGHTBLUE}                        "
    echo -e "${LIGHTBLUE}[8]${WHITE}FULLPORTONLY SCAN MODU${LIGHTBLUE}                       "
    echo -e "${LIGHTBLUE}[9]${WHITE}PORT TARAMA MODU${LIGHTBLUE}                          " 
    echo -e "${LIGHTBLUE}[10]${WHITE}WEB MODU - SADECE PORT 80 + 443${LIGHTBLUE}                          " 
    echo -e "${LIGHTBLUE}[11]${WHITE}HTTP WEB PORT MODU${LIGHTBLUE}                          " 
    echo -e "${LIGHTBLUE}[12]${WHITE}HTTPS WEB PORT MODU${LIGHTBLUE}                         " 
    echo -e "${LIGHTBLUE}[13]${WHITE}BRUTEFORCE ENABLE${LIGHTBLUE}    " 
    echo -e "${LIGHTBLUE}[14]${WHITE}AIRSTRIKE MODU${LIGHTBLUE}              "
    echo -e "${LIGHTBLUE}[15]${WHITE}Hedef Listesi İle Nuke Modu,BruteForce Yetkili,Fullportscan Modu${LIGHTBLUE}"
    echo -e "${LIGHTBLUE}[16]${WHITE}LOOT REIMPORT FONKSIYONU --reimport${LIGHTBLUE}              "
    echo -e "${LIGHTBLUE}[17]${WHITE}LOOT REIMPORT FONKSIYONU --deroad${LIGHTBLUE}              "
    echo -e "${LIGHTBLUE}[18]${WHITE}UPDATE${LIGHTBLUE}              "
    echo -e "${RED}======================================================+ "
    echo -n "Menuden Parametreleri Secebilirsiniz [1 - 18] "
    read yourch
    case $yourch in
      1) gitKurulum ;;
      2) Kurulum ;;
      3) normalTARAMA ;;
      4) normalOSINTRECON ;;
      5) stealthMODOSINTRECON ;;
      6) kesfetmeMOD ;;
      7) OZELPORTTARAMA ;;
      8) FULLPORTONLYSCANMODU ;;
      9) PORTMODU ;;
      10) WEBMODU ;;
      11) HTTPWEBMODU ;;
      12) HTTPSWEBMODU ;;
      13) BRUTEFORCEENABLED ;;
      14) AIRSTRIKEMODU ;;
      15) TOPLUMOD ;;
      16) LOOTREIMPORTFONKSIYONU ;;
      17) LOOTREIMPORTFONKSIYONUderoad ;;
      18) UPDATE ;;
      ex) echo "As you Command" ;  exit 1  ;;
      *) echo "HATALI RAKAM GIRDINIZ" ;
         echo "Enter Basarak Menuye Dönebilirsiniz . . ." ; read ;;
 esac
done # end_while
}
#
# Set trap to for CTRL+C interrupt i.e. Install our error handler
# When occurs it first it calls del_file() and then exit
#
trap exit_script 2
#
# Call our user define function : Take_input1
#
Take_input1
