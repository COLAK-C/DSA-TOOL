#!/bin/bash
BLACK='\e[0;30m'
BLUE='\e[0;34m'
GREEN='\e[0;32m'
CYAN='\e[0;36m'
RED='\e[0;31m'
PURPLE='\e[0;35m'
BROWN='\e[0;33m'
LIGHTGRAY='\e[0;37m'
DARKGRAY='\e[1;30m'
LIGHTBLUE='\e[1;34m'
LIGHTGREEN='\e[1;32m'
LIGHTCYAN='\e[1;36m'
LIGHTRED='\e[1;31m'
LIGHTPURPLE='\e[1;35m'
YELLOW='\e[1;33m'
WHITE='\e[1;37m'
NC='\e[0m'              

exit_script()
{
cd /root/Desktop/DSA-TOOL
  ./DSA-TOOL
}

gitKurulum()
{
    clear
echo -e " ${WHITE}EMINMISINIZ . . .   "
read enter
cd /root/Desktop/DSA-TOOL/Toollar
git clone https://github.com/1N3/Sn1per.git
echo " INDIRME ISLEMI BITMISTIR ENTER'A BASARAK DEVAM EDINIZ . . .  "
read enter
}

Kurulum()
{
    clear
cd /root/Desktop/DSA-TOOL/Toollar/Sn1per
./install.sh
echo " "
echo " Enter'a Basarak Devam Edebilirsiniz . . .  "
read enter
}

normalTARAMA()
{
    clear
cd /root/Desktop/DSA-TOOL/Toollar/Sn1per
    echo " "
    echo -e "${BLUE}██████╗ ██╗██╗      ██████╗ ██╗                              "
    echo -e "${BLUE}██╔══██╗██║██║     ██╔════╝ ██║                              "
    echo -e "${BLUE}██████╔╝██║██║     ██║  ███╗██║                              "
    echo -e "${BLUE}██╔══██╗██║██║     ██║   ██║██║                              "
    echo -e "${BLUE}██████╔╝██║███████╗╚██████╔╝██║                              "
    echo -e "${BLUE}╚═════╝ ╚═╝╚══════╝ ╚═════╝ ╚═╝                              "
    echo -e "${WHITE}                                                             "
    echo -e "${WHITE}████████╗ ██████╗ ██████╗ ██╗      █████╗ ███╗   ███╗ █████╗ "
    echo -e "${WHITE}╚══██╔══╝██╔═══██╗██╔══██╗██║     ██╔══██╗████╗ ████║██╔══██╗"
    echo -e "${WHITE}   ██║   ██║   ██║██████╔╝██║     ███████║██╔████╔██║███████║"
    echo -e "${WHITE}   ██║   ██║   ██║██╔═══╝ ██║     ██╔══██║██║╚██╔╝██║██╔══██║"
    echo -e "${WHITE}   ██║   ╚██████╔╝██║     ███████╗██║  ██║██║ ╚═╝ ██║██║  ██║"
    echo -e "${WHITE}   ╚═╝    ╚═════╝ ╚═╝     ╚══════╝╚═╝  ╚═╝╚═╝     ╚═╝╚═╝  ╚═╝"
                                                             
    echo -e "${WHITE}CODED BY ÇOLAK ${GREEN}DarkSecArmy.COM | PcDunyasi.TV${RED}  [NO EDIT] "
    echo -e "${GREEN}HEDEF BELIRTINIZ ORNEK : trump.org  ${YELLOW}" 
    echo " " 
echo "HEDEF : "
read host
./sniper -t $host
echo " "
echo " Enter'a Basarak Devam Edebilirsiniz . . .  "
read enter
}

